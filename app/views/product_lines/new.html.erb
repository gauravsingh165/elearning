<h1>Create a New Product Line</h1>
<%= form_with(model: @product_line, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="actions">
    <%= form.submit 'Create Product Line' %>
  </div>
<% end %>
<%= link_to 'All Product Lines', product_lines_path %>
