<h1>All course</h1>
<% @courses.each do |course| %>
  <%= turbo_frame_tag course do %>
    <div class="course">
      <div class="card card-body mt-5"> 
        <h1><strong>Course Name:</strong><%= course.name%></h1><br>
        <h1><strong>Start Date:</strong><%=  course.start_date%></h1><br>
        <h1><strong>End Date:</strong><%=  course.end_date%></h1>
        <% if can? :create, @course %>
        <%= button_to "Delete", course_path(course), method: :delete, class: "btn btn-outline-danger" %>
        <%= link_to "Edit", edit_course_path(course), class: "btn btn-outline-info" %>
        <% end %>
    </div>
    </div>
  <% end %>
<% end %>
